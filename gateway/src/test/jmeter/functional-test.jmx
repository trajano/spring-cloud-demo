import java.util.zip.Inflater;

try {
  var accessToken = vars.get(&quot;accessToken&quot;);
  var compressedAccessTokenBytes = Base64.getUrlDecoder().decode(accessToken);
  var decompressor = new Inflater();
  decompressor.setInput(compressedAccessTokenBytes, 0, compressedAccessTokenBytes.length);
  var resultBytes = new byte[2048];
  var decompressedLength = decompressor.inflate(resultBytes);
  decompressor.end();
  var decompressed = new String(resultBytes, 0, decompressedLength, &quot;UTF-8&quot;);
  log.info(&quot;decompressed accessToken = {}&quot;, decompressed);
} catch (Exception e) {
  log.error(&quot;Error&quot;, e);
  Failure = true;
  FailureMessage = e.getMessage();
}
</stringProp>
