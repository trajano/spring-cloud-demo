      <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Parse base URI" enabled="true">
        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="script">import java.net.URI;
var baseUri = URI.create(vars.get(&quot;baseUri&quot;));
var httpProtocol = baseUri.getScheme();
var httpHost = baseUri.getHost();
var port = baseUri.getPort();
if (port == 0 &amp;&amp; httpProtocol.equals(&quot;https&quot;)) {
	port = 443;
} else if (port == 0) {
	port = 80;
}
var httpPrefix = baseUri.getPath();
vars.put(&quot;httpProtocol&quot;, httpProtocol);
vars.put(&quot;httpPort&quot;, String.valueOf(port));
if ((httpProtocol.equals(&quot;http&quot;) &amp;&amp; port != 80) || (httpProtocol.equals(&quot;https&quot;) &amp;&amp; port != 443)) {
  vars.put(&quot;httpPortBlankDefault&quot;, &quot;:&quot; + String.valueOf(port));
} else {
  vars.put(&quot;httpPortBlankDefault&quot;, &quot;&quot;);	
}
vars.put(&quot;httpHost&quot;, httpHost);
vars.put(&quot;httpPrefix&quot;, httpPrefix);
log.info(&quot;parsed {}&quot;, baseUri)</stringProp>
      </BeanShellPreProcessor>
