{
  "name": "@trajano/eslint-config",
  "version": "1.0.0",
  "main": "index.js",
  "license": "EPL-2.0",
  "private": true,
  "scripts": {
    "lint": "eslint index.js",
    "prepare": "eslint index.js"
  },
  "devDependencies": {
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "env": {
      "node": true
    },
    "root": true
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{json,md}": "prettier --write"
  },
  "dependencies": {
    "eslint": "^8.3.2",
    "eslint-config-universe": "^11.1.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "prettier": "^2.8.3",
    "prettier-plugin-jsdoc": "^0.4.2"
  }
}
