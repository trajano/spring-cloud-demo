{
  "name": "@trajano/spring-docker-auth-context",
  "version": "0.1.0",
  "description": "test",
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "homepage": "https://github.com/trajano/trajano-spring-docker-auth-context#readme",
  "bugs": {
    "url": "https://github.com/trajano/trajano-spring-docker-auth-context/issues"
  },
  "repository": "https://github.com/trajano/trajano-spring-docker-auth-context",
  "license": "EPL-2.0",
  "author": "Archimedes Trajano <developer@trajano.net> (https://github.com/trajano)",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "source": "src/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/.*"
  ],
  "scripts": {
    "bootstrap": "yarn example && yarn install",
    "example": "yarn --cwd example",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "test": "jest",
    "typescript": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn-or-npm test --findRelatedTests --passWithNoTests",
    "*.{ts,tsx,js}": "eslint --fix",
    "*.{json,md}": "prettier --write"
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "@trajano"
    ],
    "root": true
  },
  "eslintIgnore": [
    "lib/"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "preset": "@testing-library/react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@trajano/react-hooks": "2.0.1",
    "jest-environment-node": "^29.3.1",
    "js-base64": "^3.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@react-native-community/netinfo": "9.3.5",
    "@release-it/conventional-changelog": "^5.0.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.0",
    "@trajano/eslint-config": "1.0.0",
    "@types/jest": "^29.2.4",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "eslint": "^8.32.0",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "pod-install": "^0.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-builder-bob": "^0.20.3",
    "react-test-renderer": "18.1.0",
    "release-it": "^15.5.1",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "whatwg-fetch": "^3.6.2",
    "yarn-or-npm": "^3.0.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
